{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "movie_name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "platform": {
      "type": "string"
    },
    "poster": {
      "type": "string"
    },
    "actors": {
      "type": "string",
      "minLength": 1,
      "maxLength": 60
    },
    "rating": {
      "type": "string",
      "minLength": 1,
      "maxLength": 10
    },
    "release_year": {
      "type": "integer",
      "minLength": 4,
      "maxLength": 4
    },
    "user_id": {
      "type": "integer",
      "minLength": 1,
      "maxLength": 10
    }
  },
  "additionalProperties": false,
  "required": [
    "movie_name",
    "platform",
    "poster",
    "actors",
    "rating",
    "release_year",
    "user_id"
  ]
}
