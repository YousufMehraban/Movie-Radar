{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "movie_name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "poster": {
      "type": "string"
    },
    "rating": {
      "type": ["string", "number", "null"]
    },
    "release_year": {
      "type": "integer",
      "minLength": 4,
      "maxLength": 4
    },
    "user_id": {
      "type": "integer"
    },
    "imdb_id": {
      "type": "string",
      "minLength": 1,
      "maxLength": 60
    }
  },
  "additionalProperties": false,
  "required": [
    "movie_name",
    "poster",
    "rating",
    "release_year",
    "imdb_id",
    "user_id"
  ]
}
